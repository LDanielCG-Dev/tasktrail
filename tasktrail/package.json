{
  "name": "tasktrail",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "apply-migration": "concurrently --kill-others \"npx prisma generate\" \"npx prisma migrate deploy\"",
    "deploy": "node-sass --omit-source-map-url public/sass/ -o public/css/ && npm run build:prod",
    "css-build": "node-sass --omit-source-map-url public/sass/ -o public/css/",
    "css-watch": "npm run css-build -- --watch",
    "start": "concurrently --kill-others \"nodemon ./bin/www\" \"npm run css-watch\"",
    "start:prod": "concurrently --kill-others \"node ./bin/www\"",
    "build": "webpack --mode=production",
    "build:dev": "npm run start \"webpack-dev-server\"",
    "build:prod": "concurrently --kill-others \"npm run start:prod\" \"webpack serve --mode=production\""
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.6",
    "@fullcalendar/daygrid": "^6.1.6",
    "@fullcalendar/interaction": "^6.1.6",
    "@fullcalendar/list": "^6.1.8",
    "@fullcalendar/vue": "^6.1.6",
    "@prisma/client": "^4.15.0",
    "@thesameeric/fileguard": "^1.1.2",
    "@types/alertify": "^0.3.31",
    "alertifyjs": "^1.13.1",
    "bcrypt": "^5.1.0",
    "bulma": "^0.9.4",
    "concurrently": "^7.6.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.7",
    "http-errors": "~1.6.3",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.0",
    "mariadb": "^3.1.0",
    "mime": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-sass": "^8.0.0",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.1.0",
    "passport": "0.5.3",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^4.15.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "v-calendar": "^2.4.1",
    "validator": "^13.9.0",
    "vue": "^2.7.14",
    "vue-color": "^2.8.1",
    "vue-dndrop": "^1.3.0",
    "vue-i18n": "^8.28.2",
    "vue-loader": "^15.10.1",
    "vue-router": "^3.5.2",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@volar/vue-language-plugin-pug": "^1.2.0",
    "css-loader": "^6.7.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.2",
    "vue-svg-inline-plugin": "^2.2.3",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1"
  }
}
